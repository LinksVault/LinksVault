{
  "expo": {
    "name": "LinksVault",
    "slug": "linksvault-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LinksVaultLogo.png",
    "userInterfaceStyle": "light",
    "scheme": "linksvault",
    "splash": {
      "image": "./assets/LinksVaultLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.linksvault.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LinksVaultLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yonatancarmeli.linksvault",
      "navigationBar": {
        "backgroundColor": "#f5f5f5",
        "barStyle": "dark-content"
      }
    },
    "web": {
      "favicon": "./assets/LinksVaultLogo.png"
    },
    "plugins": [
      "expo-build-properties",
      "expo-navigation-bar"
    ],
    "owner": "yonatan_carmeli",
    "extra": {
      "eas": {
        "projectId": "79ce5875-f5e8-4caa-87e9-cd3227db6551"
      },
      "previewServerUrl": "http://localhost:3000"
    }
  }
}
